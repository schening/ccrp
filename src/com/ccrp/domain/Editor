package com.ccrp.domain;

import java.io.Serializable;
import java.util.Set;
/**
 * 编辑类
 * @author YaoJialing
 *
 */
public class Editor implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
    
	private long eid;//编辑ID
	private String role;//编辑角色
	
	private Set<Manuscript> manuscripts;//编辑审核的投稿
	private Set<Article> articles;//编辑发布的文章
	
	/**
	 * 属性对应的setter()和getter()方法
	 */
	
	public long getEid() {
		return eid;
	}
	public void setEid(long eid) {
		this.eid = eid;
	}
	public String getRole() {
		return role;
	}
	public void setRole(String role) {
		this.role = role;
	}
	public static long getSerialversionuid() {
		return serialVersionUID;
	}
	
	public Set<Manuscript> getManuscripts() {
		return manuscripts;
	}
	public void setManuscripts(Set<Manuscript> manuscripts) {
		this.manuscripts = manuscripts;
	}
	public Set<Article> getArticles() {
		return articles;
	}
	public void setArticles(Set<Article> articles) {
		this.articles = articles;
	}
	/**
	 * toString()方法
	 */
	@Override
	public String toString() {
		return "Editor [eid=" + eid + ", role=" + role + ", manuscripts=" + manuscripts + ", articles=" + articles
				+ "]";
	}
	
	
}
